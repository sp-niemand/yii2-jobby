{
    "name": "sp-niemand/yii2-jobby",
    "description": "Jobby wrapper for Yii2",
    "type": "library",
    "keywords": [
        "yii2", "jobby", "scheduler"
    ],
    "homepage": "https://github.com/sp-niemand/yii2-jobby",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitri Cherepovski",
            "email": "codernumber1@gmail.com",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/sp-niemand/yii2-jobby/issues"
    },
    "require": {
        "php": ">=5.4",
        "hellogerard/jobby" : "dev-master"
    },
    "suggest": {
        "yiisoft/yii2-mongodb": "Needed to use default Jobby DB model implementation",
        "fishvision/yii2-migrate": "To use migrations from modules if Yii2 can't do it yet"
    },
    "autoload": {
        "psr-4": { "jobbyDb\\": "src/" }
    }
}
